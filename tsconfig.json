{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2015", "es2016", "dom", "es2017.object"],
    "module": "es2015",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "strict": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "suppressImplicitAnyIndexErrors": true,
    "removeComments": true,
    "baseUrl": ""
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules"]
}